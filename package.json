{
  "name": "@ory/integration-react",
  "version": "0.0.1",
  "config": {
    "prettierTarget": "**/*.{tsx,ts,json,md,js,css}"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
    "format:check": "prettier --check ${npm_package_config_prettierTarget}",
    "build:lib": "rm -rf dist && rollup -c",
    "prepublishOnly": "npm run build:lib",
    "test": "cypress run",
    "test:dev": "cypress open"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@ory/client": "^1.15.0",
    "@ory/integrations": "^1.2.1",
    "@ory/themes": "^0.0.103",
    "classnames": "^2.5.1",
    "next": "^14.2.11",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "react-toastify": "^10.0.5",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "react-dom": "18.3.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "~18.3.5",
    "@types/request": "^2.48.12",
    "@types/styled-components": "^5.1.34",
    "cypress": "^13.14.2",
    "eslint": "9.10.0",
    "eslint-config-next": "14.2.11",
    "ory-prettier-styles": "^1.3.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.3",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}
